        </note>
        
        <roleset id="coagulate.01" name="(cause to) convert liquid to solid"
 vncls="45.4">
            <roles>
                <role descr="cause of coagulation" n="0">
                    <vnrole vncls="45.4" vntheta="Agent"/>
                </role>
                <role descr="entity ionized" n="1">
                    <vnrole vncls="45.4" vntheta="Patient"/>
                </role>
                <role descr="instrument or manner, if separate from arg 0" n="2">
