        </note>
        
        <roleset id="awake.01" name="to wake or make aware" vncls="45.4 48.1.1">
            <roles>
                <role descr="entity awake" n="1">
                    <vnrole vncls="45.4"
  vntheta="Agent"/><vnrole vncls="48.1.1" vntheta="Theme"/>
                </role>
                <role descr="entity becoming aware of" n="2">
                    <vnrole vncls="45.4" vntheta="Patient"/>
