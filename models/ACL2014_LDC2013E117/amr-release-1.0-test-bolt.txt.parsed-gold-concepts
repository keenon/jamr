# ::edge	name	op1	"China"	0.1.0.0.1	0.1.0.0.1.0	
# ::edge	person	mod	cheap	0.1.1.0	0.1.1.0.0	
# ::edge	repute-01	ARG1	feudal	0	0.0	
# ::edge	repute-01	ARG2	thing	0	0.1	
(r / repute-01 
      :ARG1 (f / feudal) 
      :ARG2 (t2 / thing 
            :ARG1-of (a / agree-01 
                  :ARG0 (c2 / country 
                        :ARG0-of (m2 / make-01 
                              :ARG1 (s / sense) 
                              :ARG6 (s2 / somewhat)) 
                        :name (n / name 
                              :op1 "China")) 
                  :ARG6 (j / just)) 
            :ARG1-of (e / entertain-01 
                  :ARG0 (p / person 
                        :mod (c / cheap)))))

